<%= link_to("<< Back to List", type_message_index_path, :class => 'back-link') %>

<div class="type_message edit">
  <h2>Update type_message</h2>

  <%= form_for(@type_message) do |f| %>

    <table summary="type_message form fields">
      <tr>
        <th>Name</th>
        <td><%= f.text_field(:name) %></td>
      </tr>
    </table>

    <div class="form-buttons">
      <%= f.submit("Update type_message") %>
    </div>

  <% end %>
</div>